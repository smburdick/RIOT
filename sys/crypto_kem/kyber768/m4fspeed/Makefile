
CC=arm-none-eabi-gcc

all:
    $(CC) cbd.c fips202.c indcpa.c kem.c matacc.c ntt.c poly.c polyvec.c randombytes.c symmetric-fips202.c verify.c -o kyber.o

arm-none-eabi-gcc -mcpu=cortex-m4 -mthumb -mfloat-abi=hard -mfpu=fpv4-sp-d16 -std=gnu99 --sysroot= -I/home/sam/workspace/pqm4/common -I/home/sam/workspace/pqm4/mupq/common -O3 -g3 -Wall -Wextra -Wshadow -MMD -fno-common -DSTM32F4 -DSTM32F4CCM -DSTM32F407VG -I/home/sam/workspace/pqm4/libopencm3/include -DMUPQ -DMUPQ_NAMESPACE= -DMUPQ_ITERATIONS=1 -DPQM4 -Icrypto_kem/kyber768-90s/m4fspeed -L/home/sam/workspace/pqm4/libopencm3/lib --specs=nosys.specs -Wl,--wrap=_sbrk -nostartfiles -ffreestanding -Tldscripts/stm32f4discovery.ld -mcpu=cortex-m4 -mthumb -mfloat-abi=hard -mfpu=fpv4-sp-d16 -Lobj -Wl,--gc-sections -o elf/crypto_kem_kyber768-90s_m4fspeed_test.elf mupq/crypto_kem/test.c common/hal-opencm3.c common/randombytes.c mupq/common/fips202.c mupq/common/sp800-185.c mupq/common/nistseedexpander.c common/keccakf1600.S common/aes.c common/aes-encrypt.S common/aes-keyschedule.S common/aes-publicinputs.c common/aes-publicinputs.S mupq/common/sha2.c common/crypto_hashblocks_sha512_inner32.s common/crypto_hashblocks_sha512.c crypto_kem/kyber768-90s/m4fspeed/aes256ctr.c crypto_kem/kyber768-90s/m4fspeed/cbd.c crypto_kem/kyber768-90s/m4fspeed/indcpa.c crypto_kem/kyber768-90s/m4fspeed/kem.c crypto_kem/kyber768-90s/m4fspeed/matacc.c crypto_kem/kyber768-90s/m4fspeed/ntt.c crypto_kem/kyber768-90s/m4fspeed/poly.c crypto_kem/kyber768-90s/m4fspeed/polyvec.c crypto_kem/kyber768-90s/m4fspeed/verify.c crypto_kem/kyber768-90s/m4fspeed/fastaddsub.S crypto_kem/kyber768-90s/m4fspeed/fastbasemul.S crypto_kem/kyber768-90s/m4fspeed/fastinvntt.S crypto_kem/kyber768-90s/m4fspeed/fastntt.S crypto_kem/kyber768-90s/m4fspeed/matacc_asm.S crypto_kem/kyber768-90s/m4fspeed/poly_asm.S crypto_kem/kyber768-90s/m4fspeed/reduce.S -Wl,--start-group -lc -lgcc -lopencm3_stm32f4 -lm -Wl,--end-group
